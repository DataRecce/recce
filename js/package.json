{
  "name": "recce",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && cp -R out/. ../recce/data/ && rm -rf out",
    "start": "next start",
    "lint": "biome check --diagnostic-level=error",
    "lint:fix": "biome check --write --diagnostic-level=error",
    "lint:staged": "biome check --staged --diagnostic-level=error",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "type:check": "tsc --noEmit",
    "clean": "rimraf ../recce/data",
    "prepare": "cd .. && husky js/.husky"
  },
  "dependencies": {
    "@amplitude/unified": "1.0.0-beta.9",
    "@babel/helpers": "^7.28.6",
    "@babel/runtime": "^7.28.6",
    "@codemirror/lang-sql": "^6.10.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/merge": "^6.11.2",
    "@codemirror/state": "^6.5.4",
    "@codemirror/view": "^6.39.11",
    "@dagrejs/dagre": "^1.1.8",
    "@datarecce/ui": "workspace:*",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/montserrat": "^5.2.8",
    "@hello-pangea/dnd": "^18.0.1",
    "@jeromefitz/date-fns-tz": "3.0.0-canary.8",
    "@mui/material": "^7.3.7",
    "@mui/system": "^7.3.7",
    "@next/third-parties": "16.1.3",
    "@sentry/nextjs": "^10.32.1",
    "@sentry/react": "^10.32.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.18",
    "@types/js-cookie": "^3.0.6",
    "@uiw/codemirror-theme-github": "^4.25.4",
    "@uiw/react-codemirror": "^4.25.4",
    "@xyflow/react": "^12.10.0",
    "ag-grid-community": "^35.0.0",
    "ag-grid-react": "^35.0.0",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "codemirror": "^6.0.2",
    "common-tags": "^1.8.2",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "happy-dom": "^20.3.1",
    "html-to-image": "^1.11.13",
    "html2canvas-pro": "^1.6.4",
    "import-in-the-middle": "^2.0.4",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "next": "16.1.3",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-select": "^5.10.2",
    "react-split": "^2.0.14",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.1",
    "require-in-the-middle": "^8.0.1",
    "simplebar": "^6.3.3",
    "simplebar-react": "3.3.2",
    "usehooks-ts": "^3.1.1",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@amplitude/analytics-core": "^2.34.1",
    "@biomejs/biome": "2.3.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/common-tags": "^1.8.4",
    "@types/dagre": "^0.7.53",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.0.9",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "baseline-browser-mapping": "^2.9.15",
    "fast-check": "^4.5.3",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "pnpm": {
    "overrides": {
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
      "js-yaml@<3.14.2": ">=3.14.2",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0"
    },
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "sharp",
      "unrs-resolver"
    ]
  },
  "overrides": {
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3"
  }
}
