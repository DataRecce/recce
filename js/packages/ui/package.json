{
  "name": "@datarecce/ui",
  "version": "0.2.1",
  "license": "Apache-2.0",
  "author": "Recce Dev Team <dev@reccehq.com>",
  "description": "Recce UI Components Library - Reusable React components for data validation and review",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DataRecce/recce.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js",
      "default": "./dist/api.js"
    },
    "./primitives": {
      "types": "./dist/primitives.d.ts",
      "import": "./dist/primitives.js",
      "default": "./dist/primitives.js"
    },
    "./advanced": {
      "types": "./dist/advanced.d.ts",
      "import": "./dist/advanced.js",
      "default": "./dist/advanced.js"
    },
    "./contexts": {
      "types": "./dist/contexts.d.ts",
      "import": "./dist/contexts.js",
      "default": "./dist/contexts.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.js",
      "default": "./dist/hooks.js"
    },
    "./result": {
      "types": "./dist/result.d.ts",
      "import": "./dist/result.js",
      "default": "./dist/result.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "default": "./dist/types.js"
    },
    "./styles": {
      "default": "./dist/styles.css"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "default": "./dist/utils.js"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "import": "./dist/theme.js",
      "default": "./dist/theme.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js",
      "default": "./dist/constants.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js",
      "default": "./dist/components.js"
    },
    "./components/run": {
      "types": "./dist/components-run.d.ts",
      "import": "./dist/components-run.js",
      "default": "./dist/components-run.js"
    },
    "./lib/const": {
      "types": "./dist/lib/const.d.ts",
      "import": "./dist/lib/const.js",
      "default": "./dist/lib/const.js"
    },
    "./lib/api/track": {
      "types": "./dist/lib/api/track.d.ts",
      "import": "./dist/lib/api/track.js",
      "default": "./dist/lib/api/track.js"
    },
    "./lib/api/user": {
      "types": "./dist/lib/api/user.d.ts",
      "import": "./dist/lib/api/user.js",
      "default": "./dist/lib/api/user.js"
    },
    "./lib/api/axiosClient": {
      "types": "./dist/lib/api/axiosClient.d.ts",
      "import": "./dist/lib/api/axiosClient.js",
      "default": "./dist/lib/api/axiosClient.js"
    },
    "./lib/result/ResultErrorFallback": {
      "types": "./dist/lib/result/ResultErrorFallback.d.ts",
      "import": "./dist/lib/result/ResultErrorFallback.js",
      "default": "./dist/lib/result/ResultErrorFallback.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "*.css"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@amplitude/analytics-core": "^2.34.1",
    "@amplitude/unified": "1.0.0-beta.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.0",
    "@mui/system": "^7.0.0",
    "@tanstack/react-query": "^5.0.0",
    "@xyflow/react": "^12.0.0",
    "axios": "^1.0.0",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "html-to-image": "^1.11.11",
    "html2canvas-pro": "^1.5.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "next-themes": "^0.4.0",
    "react-icons": "^5.5.0",
    "react-split": "^2.0.14",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "postcss": "^8.5.0",
    "tailwindcss": "^4.0.0",
    "typedoc": "^0.28.12",
    "typedoc-plugin-markdown": "^4.9.0",
    "tsdown": "^0.18.0",
    "typescript": "^5.9.0"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsdown && pnpm build:styles",
    "build:styles": "node scripts/build-styles.js",
    "docs:api": "typedoc --options typedoc.json",
    "type:check": "tsc --noEmit"
  }
}
